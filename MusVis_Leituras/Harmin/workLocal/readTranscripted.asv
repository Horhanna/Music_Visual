function readTranscripted(fileName)
fid = fopen(fileName, 'rt');
oFile = fopen('transcrito.txt','w');
y = 0;
while feof(fid) == 0
    y = y + 1;
    tline = fgetl(fid); 
    %disp(tline);
    if (strcmp(tline, 'D/F#'))
        disp('im here');
    end
    numLettersRoot = 1;
    accident = 0;
    if (strcmp(tline, 'No'))
        fprintf(oFile, '61 \n');
        continue;
    elseif(strcmp(tline, 'garbage'))
        continue;
    elseif(length(tline) > 1)
        if(tline(2) == '#' )
            numLettersRoot = 2;
            accident = 1;
        elseif(tline(2) == 'b' )
            numLettersRoot = 2;
            accident = -1;
        end
    end
    switch (tline(1))
        case 'C'
            root = 0;
        case 'D'
            root = 2;
        case 'E'
            root = 4;
        case 'F'
            root = 5;
        case 'G'
            root = 7;
        case 'A'
            root = 9;
        case 'B'
            root = 11;
        otherwise
            disp('something strange with the root note');
    end
    root = root + accident;
    typeStr = tline(numLettersRoot+1:length(tline));
    if(length(typeStr) == 0)
        type = 0; %'' did not work
    else
        seventh = 0;        
        [typeStr, rest] = strtok(typeStr, '/');
        barInd = findstr('/',tline);
        if(length(barInd) > 0)
            if(barInd(1) == numLettersRoot + 1)
                rest = ['
                typeStr = '';
            end
        end
        if(length(rest) > 1)
            interval = compInterval(root, rest);
            if(interval == 10)
                seventh = 1;
            end
        end
        
        switch(typeStr)
            case {'', '7M', '7M(9)','6','6/9', '4','7M(#11)'}
                type = 0 + seventh;
            case {'7','7/4','7(b9)','7(9)','7(#9)','7(13)','7(b13)','7(b9','7(b5)','7(#11)','7(#5)','7(9'}
                type = 1;
            case {'m', 'm6', 'm(b6)'}
                type = 2 + seventh;
            case {'m7', 'm7(b5)','m7(9)','m7(11)','m7(b5'}
                type = 3;
            case {'°','°(b13)'}
                type = 4;
            otherwise
                disp(typeStr);
                disp(y);
                pause;
        end
    end
end
fclose(fid); 
fclose(oFile);

function i = compInterval(root, invStr)
    accident = 0;
    if(length(invStr) > 2)
        if(invStr(3) == '#' )
            accident = 1;
        elseif(invStr(3) == 'b' )
            accident = -1;
        end
    end
    switch (invStr(2))
        case 'C'
            inv = 0;
        case 'D'
            inv = 2;
        case 'E'
            inv =  4;
        case 'F'
            inv =  5;
        case 'G'
            inv =  7;
        case 'A'
            inv =  9;
        case 'B'
            inv =  11;
        otherwise
            %disp('something strange with the inversion');
            %disp(invStr);
            i = 0;
            return;
    end
    inv = inv + accident;
 i = mod(inv - root, 12);